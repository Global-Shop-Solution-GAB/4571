Program.Sub.LoadDtOverdue.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare

	V.Local.sSQL.Declare
	V.Local.sFilter.Declare
	V.Local.dCutoffDate.Declare

	Gui.FrmAging.lblStatus.Caption(V.Translation.307302)

	F.Intrinsic.Control.If(V.DataTable.dtOverdue0.Exists)
		F.Data.DataTable.Close("dtOverdue0")
		F.Data.DataTable.Close("dtOverdue")
		F.Data.Dictionary.Close("dTranslation2")
		Gui.FrmAging.GSGCOverdue.ClearGrouping("gvOverdue")
	F.Intrinsic.Control.EndIf
	
	V.Local.dCutoffDate.Set(V.Screen.FrmAging!dtpAgeDate.Value)

	F.Intrinsic.String.Build("Select Distinct A.Customer,RTrim(B.Name_Customer) As Name_Customer,Case When RTrim(D.Location) Is Null Then '' Else RTrim(D.Location) End As Location,LTrim(RTrim(A.Salesperson)) As SP_ID,LTrim(RTrim(E.Name)) As Salesperson,A.Invoice,(Select Min(Date_Invoice) From V_AR_Open_Items Where Customer = A.Customer And Invoice = A.Invoice And Due_Date <= '{0}' And Date_Transaction <= '{0}' And Batch_Code In ('10')) As Date_Invoice,(Select Min(Due_Date) From V_AR_Open_Items Where Customer = A.Customer And Invoice = A.Invoice And Due_Date <= '{0}' And Date_Transaction <= '{0}' And Batch_Code In ('10')) As Due_Date,(Select Sum(Amt_Invoice) From V_AR_Open_Items Where Customer = A.Customer And Invoice = A.Invoice And Due_Date <= '{0}' And Date_Transaction <= '{0}') As Amt_Invoice,Convert(0,SQL_INTEGER) AS Days_Late,'' As note_1,'452800' As Print_Invoice,'452800' As Print_Statement,'683000' As Open_Item_Notes,'206600' As Email_Invoice From V_AR_Open_Items A Left Join V_Customer_Master B On A.Customer = B.Customer Left Join V_Order_Hist_Head D On A.Invoice = D.Invoice Left Join V_Salespersons E On A.Salesperson = E.ID Where Amt_Invoice > 0 Order By A.Customer,Invoice;",V.Local.dCutoffDate.PervasiveDate,V.Local.sSQL)
	F.Data.DataTable.CreateFromSQL("dtOverdue0","con",V.Local.sSQL,True)
	'Set Button values based on translation
	F.Data.Dictionary.Create("dTranslation2")
	F.Data.Dictionary.AddItem("dTranslation2","452800",V.Translation.452800)
	F.Data.Dictionary.AddItem("dTranslation2","683000",V.Translation.683000)
	F.Data.Dictionary.AddItem("dTranslation2","206600",V.Translation.206600)
	F.Data.DataTable.FillFromDictionary("dtOverdue0","dTranslation2","Print_Invoice")
	F.Data.DataTable.FillFromDictionary("dtOverdue0","dTranslation2","Print_Statement")
	F.Data.DataTable.FillFromDictionary("dtOverdue0","dTranslation2","Open_Item_Notes")
	F.Data.DataTable.FillFromDictionary("dtOverdue0","dTranslation2","Email_Invoice")
	'Add Notes from Dictionary
	F.Data.Dictionary.CreateFromSQL("dNotes","con","Select Invoice,Note_1 From GAB_4571_OVDUE_INVC;")
	F.Data.Dictionary.SetDefaultReturn("dNotes","")
	F.Data.DataTable.FillFromDictionary("dtOverdue0","dNotes","Invoice","note_1")
	F.Data.Dictionary.Close("dNotes")

	'Set DataView Security
	F.Intrinsic.Control.If(V.Global.bAdmin,=,False)
		F.Intrinsic.Control.If(V.Global.sLoc,=,"")
		F.Intrinsic.Control.AndIf(V.Global.sSPID,<>,"")
			F.Intrinsic.String.Build("SP_ID = '{0}' And Due_Date <= #{1}#",V.Global.sSPID,V.Local.dCutoffDate.FormatMM/DD/YYYY,V.Local.sFilter)
		F.Intrinsic.Control.ElseIf(V.Global.sLoc,<>,"")
		F.Intrinsic.Control.AndIf(V.Global.sSPID,=,"")
			F.Intrinsic.String.Build("LOCATION = '{0}' And Due_Date <= #{1}#",V.Global.sLoc,V.Local.dCutoffDate.FormatMM/DD/YYYY,V.Local.sFilter)
		F.Intrinsic.Control.ElseIf(V.Global.sLoc,<>,"")
		F.Intrinsic.Control.AndIf(V.Global.sSPID,<>,"")
			F.Intrinsic.String.Build("SP_ID = '{0}' AND LOCATION = '{1}' And Due_Date <= #{2}#",V.Global.sSPID,V.Global.sLoc,V.Local.dCutoffDate.FormatMM/DD/YYYY,V.Local.sFilter)
		F.Intrinsic.Control.ElseIf(V.Global.sLoc,=,"")
		F.Intrinsic.Control.AndIf(V.Global.sSPID,=,"")
			F.Intrinsic.UI.Msgbox(V.Translation.653901,V.Translation.429400)
			F.Intrinsic.Control.End 
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Build("Due_Date <= #{0}#",V.Local.dCutoffDate.FormatMM/DD/YYYY,V.Local.sFilter)
	F.Intrinsic.Control.EndIf

	F.Data.DataView.Create("dtOverdue0","dvOverdue0",22,V.Local.sFilter,"")
	F.Data.DataView.ToDataTable("dtOverdue0","dvOverdue0","dtOverdue",True)
	F.Data.DataView.Close("dtOverdue0","dvOverdue0")

	F.Data.DataTable.RemoveColumn("dtOverdue","SP_ID")
	F.Intrinsic.Control.CallSub(SetOverdueTotals)
	F.Intrinsic.Control.CallSub(LoadGvOverdue)
	F.Intrinsic.Control.CallSub(total,"Filter",V.Local.sFilter)

	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.LoadDtOverdue.End

Program.Sub.SetOverdueTotals.Start
	V.Local.iCnt.Declare
	V.Local.iCnt2.Declare
	V.Local.iCnt3.Declare
	V.Local.sFilter.Declare
	V.Local.fRet.Declare
	V.Local.sLocation.Declare
	V.Local.sLocationTrim.Declare
	V.Local.sSalesperson.Declare
	V.Local.sSalespersonTrim.Declare
	
	F.Data.DataView.Create("dtOverdue","dvOverdueLoc")
	F.Data.DataView.ToDataTableDistinct("dtOverdue","dvOverdueLoc","dtLoc","Location",True)
	F.Data.DataView.Close("dtOverdue","dvOverdueLoc")
	F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtLoc.RowCount--,1)
		F.Intrinsic.String.Build("Location = '{0}'",V.DataTable.dtLoc(V.Local.iCnt).Location!FieldVal,V.Local.sFilter)
		F.Data.DataView.Create("dtOverdue","dvOverdueSP",22,V.Local.sFilter,"")
		F.Data.DataView.ToDataTableDistinct("dtOverdue","dvOverdueSP","dtSP","Location*!*Salesperson",True)
		F.Data.DataView.Close("dtOverdue","dvOverdueSP")
		'Location Total\Grand Total
		F.Intrinsic.String.Build("z{0}",V.DataTable.dtLoc(V.Local.iCnt).Location!FieldVal,V.Local.sLocation)
		F.Data.DataTable.Compute("dtOverdue","Sum(Amt_Invoice)",V.Local.sFilter,V.Local.fRet)
		F.Data.DataTable.AddRow("dtOverdue","Location",V.DataTable.dtLoc(V.Local.iCnt).Location!FieldVal,"Salesperson",V.Local.sLocation,"Customer",V.Local.sLocation,"Invoice",V.Translation.268800,"Amt_Invoice",V.Local.fRet)
		F.Intrinsic.Control.For(V.Local.iCnt2,0,V.DataTable.dtSP.RowCount--,1)
			F.Intrinsic.String.Build("Location = '{0}' And Salesperson = '{1}'",V.DataTable.dtLoc(V.Local.iCnt).Location!FieldVal,V.DataTable.dtSP(V.Local.iCnt2).Salesperson!FieldVal,V.Local.sFilter)
			F.Data.DataView.Create("dtOverdue","dvOverdueCust",22,V.Local.sFilter,"")
			F.Data.DataView.ToDataTableDistinct("dtOverdue","dvOverdueCust","dtCust","Location*!*Salesperson*!*Customer*!*Name_Customer",True)
			F.Data.DataView.Close("dtOverdue","dvOverdueCust")
			'Salesperson Total
			F.Intrinsic.String.Build("z{0}",V.DataTable.dtSP(V.Local.iCnt2).Salesperson!FieldVal,V.Local.sSalesperson)
			F.Data.DataTable.Compute("dtOverdue","Sum(Amt_Invoice)",V.Local.sFilter,V.Local.fRet)
			F.Data.DataTable.AddRow("dtOverdue","Location",V.DataTable.dtLoc(V.Local.iCnt).Location!FieldVal,"Salesperson",V.DataTable.dtSP(V.Local.iCnt2).Salesperson!FieldVal,"Name_Customer",V.Local.sSalesperson,"Customer",V.Local.sSalesperson,"Invoice",V.Translation.545500,"Amt_Invoice",V.Local.fRet)
			F.Intrinsic.Control.For(V.Local.iCnt3,0,V.DataTable.dtCust.RowCount--,1)
				'Customer Total
				F.Intrinsic.String.Build("Location = '{0}' And Salesperson = '{1}' And Customer = '{2}'",V.DataTable.dtLoc(V.Local.iCnt).Location!FieldVal,V.DataTable.dtSP(V.Local.iCnt2).Salesperson!FieldVal,V.DataTable.dtCust(V.Local.iCnt3).Customer!FieldVal,V.Local.sFilter)
				F.Data.DataTable.Compute("dtOverdue","Sum(Amt_Invoice)",V.Local.sFilter,V.Local.fRet)
